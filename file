function getFrontActionLink($pagename,$display_name="",$class="",$id_name="",$id="",$title="",$extra="")
{

	if(SITE_URL_REWRITE!='On')
	{ 
		################ For PHP Link Setting ##########################
		if(!empty($id))
		{
			$getid='&'.$id_name.'='.$id;
		}
		
		$link='<a href="'.FRT_FOLDER_WWW.FRT_INDEX_PARAMETER.$pagename.$getid.'" class="'.$class.'" title="'.$title.'" '.$extra.'><span>'.$display_name.'</span></a>';
		
	}
	else
	{  
		if(!empty($id))
		{
			$getid='&'.$id_name.'='.$id;
		}
	   	$link='<a href="'.FRT_FOLDER_WWW.$pagename.$getid.'" class="'.$class.'" title="'.$title.'"'.$extra.'><span>'.$display_name.'</span></a>';
       
	}
	##################################################################
	
	return $link;	
}

