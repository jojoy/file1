function getPageName($filename,$field_prefix,$language_prefix="")
{
	/*if($language_prefix == FRT_ENGLISH_PREFIX)
	{
		$language_prefix = FRT_DUTCH_PREFIX;
	}
	else
	{
		$language_prefix = FRT_ENGLISH_PREFIX;
	}*/
	
	$newFileName = explode(".",$filename);
	$newname = $newFileName[0];
	
	$query = "SELECT * FROM " .TBL_FRONT_MENU." WHERE ".$field_prefix."id!=0 ";
	$query .=" and ".$field_prefix."filename".$language_prefix."='".$newname."'";
	$query .=" and ".$field_prefix."status='Active'";
	
	return $query;
}

function getPageNameLanguage($filename,$language_prefix="")
{
	
	if($language_prefix == FRT_DUTCH_PREFIX)
	{
		$language_prefix = FRT_DUTCH_PREFIX;
	}
	else if($language_prefix == FRT_FRENCH_PREFIX)
	{
		$language_prefix = FRT_FRENCH_PREFIX;
	}
	else
	{
		$language_prefix = FRT_ENGLISH_PREFIX;
	}	
	$newFileName = explode(".",$filename);
	$newname = $newFileName[0];
	
	$query = "SELECT * FROM " .TBL_FRONT_MENU." WHERE fmnu_id!=0 ";
	$query .=" and ".$field_prefix."fmnu_page".$language_prefix."='".$newname."'";
	$query .=" and fmnu_status='Active'";
	
	return $query;
}
function getInnerPageName($filename,$language_prefix="")
{	
	if($language_prefix == FRT_ENGLISH_PREFIX)
	{
		$language_prefix = FRT_DUTCH_PREFIX;
	}
	else
	{
		$language_prefix = FRT_ENGLISH_PREFIX;
	}
	
	$newFileName = explode(".",$filename);
	$newname = $newFileName[0];
	$query = "SELECT * FROM " .TBL_LINK_PAGE_NAME." WHERE link_id!=0 ";
	$query .=" and link_name".$language_prefix."='".$newname."'";
	return $query;
}

function getFrontPageTitle($filename,$field_prefix)
{
	global $db;
	
	if(!empty($filename))
	{	
		/*if($_SESSION['frt_language_prefix'] == FRT_ENGLISH_PREFIX)
		{
			$language_prefix = FRT_DUTCH_PREFIX;
		}
		else
		{
			$language_prefix = FRT_ENGLISH_PREFIX;
		}*/

		$getname = getPageName($filename,$field_prefix,$language_prefix);

		$res_data = $db->select($getname,2);
		
		if(count($res_data) > 0)
		{
			if($_SESSION['frt_language_prefix'] == FRT_DUTCH_PREFIX)		
			{
				$pageTitle = $res_data[0]['page_title_du'];
				
			}
			else
			{
				$pageTitle = $res_data[0]['page_title'];				
			}
		}		
	}
	return $pageTitle;
}
function getFrontHeadingTitle($filename,$field_prefix)
{
	global $db;
	
	if(!empty($filename))
	{	
		/*if($_SESSION['frt_language_prefix'] == FRT_ENGLISH_PREFIX)
		{
			$language_prefix = FRT_DUTCH_PREFIX;
		}
		else
		{
			$language_prefix = FRT_ENGLISH_PREFIX;
		}*/
		
		$getname = getPageName($filename,$field_prefix,$language_prefix);
		$res_data = $db->select($getname,2);
		
		if(count($res_data) > 0)
		{
			if($_SESSION['frt_language_prefix'] == FRT_DUTCH_PREFIX)		
			{
				$headingTitle = $res_data[0]['heading_title_du'];
				
			}
			else
			{
				$headingTitle = $res_data[0]['heading_title'];				
			}
		}		
	}
	return $headingTitle;
}
function getFrontPageBreadcrumbs($filename,$field_prefix)
{
	global $db;
	
	if(!empty($filename))
	{	
		/*if($_SESSION['frt_language_prefix'] == FRT_ENGLISH_PREFIX)
		{
			$language_prefix = FRT_DUTCH_PREFIX;
		}
		else
		{
			$language_prefix = FRT_ENGLISH_PREFIX;
		}*/
		
		$getname = getPageName($filename,$field_prefix,$language_prefix);
		$res_data = $db->select($getname,2);
		if(count($res_data) > 0)
		{
			if($_SESSION['frt_language_prefix'] == FRT_DUTCH_PREFIX)		
			{
				$breadcrumbs = $res_data[0]['breadcrumbs_du'];
				
			}
			else
			{
				$breadcrumbs = $res_data[0]['breadcrumbs'];				
			}
		}
	}
	return $breadcrumbs;
}
################################################################################################################################


################################################################################################################################
function replaceHtaccessParameter($parameter)
{
	$replace = str_replace(" ","-",strtolower($parameter));
	$replace = str_replace("/","-",$replace);
	
	return $replace;
}
function getReplaceDatabaseTitleField($fieldname)
{
	$replaceTitle = "REPLACE(LOWER(REPLACE(".$fieldname.",' ','-')),'/','-')";
	
	return $replaceTitle;
}
################################################################################################################################


################################################################################################################################
function deleteUploadImage($image_name,$section_name,$extra="")
{
	if($section_name=='News')
	{
		$upload_path=NEWS_IMG_UPLOAD;
        $org_img_prefix=NEWS_IMG_ORG_PREFIX;
		$th_img_prefix=NEWS_IMG_TH_PREFIX;
	}	
	if($section_name=='Portfolio')
    {
        $upload_path=PORFOLIO_THUMBNAIL_IMG_UPLOAD;
    	$org_img_prefix=PORFOLIO_IMG_ORG_PREFIX;
    	$th_img_prefix=PORFOLIO_IMG_TH_PREFIX;
    }
	if($section_name=="Portfolio_image") {
		$upload_path=PORFOLIO_IMG_UPLOAD;
    	$org_img_prefix=PORFOLIO_IMG_ORG_PREFIX;
    	$th_img_prefix=PORFOLIO_IMG_TH_PREFIX;
	}
	//echo "===>".$upload_path.$org_img_prefix.$image_name;
     //exit;
	if(file_exists($upload_path.$org_img_prefix.$image_name))
	{
        
        unlink($upload_path.$org_img_prefix.$image_name);
	}
	if(file_exists($upload_path.$th_img_prefix.$image_name))
	{
		unlink($upload_path.$th_img_prefix.$image_name);
	}
}

function deleteUploadFiles($files_name,$section_name,$extra="")
{
	if($section_name=='Aanbod')
	{
		$upload_path=AANBOD_PDF_UPLOAD;
	}
    if($section_name=='Duurzaam')
    {
        $upload_path=DUURZAAM_PIC_UPLOAD;
    }
    if($section_name=='News')
	{
		$upload_path=NEWS_IMG_UPLOAD;
	}
	if($section_name=='Prijzen')
	{
		$upload_path=PRIJZEN_PDF_UPLOAD;
	}
    if($section_name=='Architectuur')
	{
		$upload_path=ARCHITEC_PDF_UPLOAD;
	}
    //echo  $upload_path.$files_name;

	if(file_exists($upload_path.$files_name))
	{
		$res = unlink($upload_path.$files_name);
	}
}

