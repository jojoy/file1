function getTextArea($field_name='',$field_id='',$field_value='',$rows='3',$cols='53')
{
	$tinymce = '<textarea name="'.$field_name.'" id="'.$field_id.'" rows="'.$rows.'" cols="'.$cols.'">'.$field_value.'</textarea>';
	
	return $tinymce;
}

function getFCKeditorSource($field_name,$field_value="")
{ 
	if(file_exists(FCK_EDITOR_DIR."fckeditor.php"))
	{
		require_once(FCK_EDITOR_DIR."fckeditor.php");
		
		$oFCKeditor = new FCKeditor($field_name);
		$oFCKeditor->BasePath = FCK_EDITOR_WWW;						
		$oFCKeditor->Config['SkinPath'] = FCK_EDITOR_SKINS;		
		$oFCKeditor->Value = stripslashes($field_value);
		$oFCKeditor->ToolbarSet	= 'Default' ;
		$oFCKeditor->Width	= 560;
		$oFCKeditor->Height	= 300;
		$oFCKeditor->Create();
	}
	else
	{
	?>
		<textarea rows="3" cols="53" name="<?=$field_name?>"><?php echo stripslashes($field_value); ?></textarea>
	<?php
	}									 
}

function getTinyMCE($name="",$value="")
{
	$tiny_mce = '<textarea name="'.$name.'" style="width:100%" class="myBasicEditor" >'.$value.'</textarea>';
	return $tiny_mce;
}


function pagingPageNumber($filename,$pages,$page)
{
	if($pages>1)
	{
		$location=$filename."&page=";		
		echo '<tr>';
			echo '<td align="right" valign="top">';
				echo '<b>Pages:<b>&nbsp;';
				echo "<select name='page_cmd' id='page_cmd' class='page_cmd' onchange='window.location=\"".$location."\"+this.value'>";
				for($t=1; $t<=$pages; $t++)
				{
					if($page==$t) {$selected='selected';}
					else {$selected='';}
					echo "<option value='".$t."' ".$selected.">".$t."</option>";
				}
				echo "</select>";	
		   echo '</td>';
	   echo '</tr>';
	   echo '<tr>';
			echo '<td height="5px"></td>';
	   echo '</tr>';
	}
}
