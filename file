################################################################################################################################


################################################################################################################################
function getSelectList($table_name,$field_prefix,$auto_id="",$status="",$xtra_subquery="")
{
	$query = "SELECT * ";
	if(!empty($xtra_subquery))
	{
		$query .=",".$xtra_subquery;
	}
	$query .=" FROM " .$table_name; 
	$query .=" WHERE ".$field_prefix."id!=0 ";
	if(!empty($auto_id))
	{
		$query .= " AND ".$field_prefix."id ='".$auto_id."'";
	}
	if(!empty($status))
	{
		$query .= " AND ".$field_prefix."status ='".$status."'";
	}   
	return $query;
}

function getViewUserList($adm_id="")
{
	$query = "SELECT * from " .TBL_ADMIN_USER." where adm_id!=0 ";
	if(!empty($adm_id))
	{
		$query .= " and adm_id = '".$adm_id."'";
	}
	return $query;
}

function getContactListWithSupplier()
{
	$sql_query = "SELECT sup.sup_fname,con.* 
				FROM ".TBL_CONTACT_US." con 	  
				LEFT JOIN ".TBL_SUPPLIER_LEADS." as supled ON (con.con_id = supled.led_con_id)	 
				LEFT JOIN ".TBL_SUPPLIER." as sup ON (sup.sup_id = supled.led_sup_id)
				WHERE con.con_id != 0 ";
				
	return $sql_query;			
}

################################################################################################################################

function getProductForMail($product)
{
	
	global $db;
	$proDiv = '';
	for($i=0;$i<count($product);$i++)
	{
		$subpro = explode("_",$product[$i]);
		$r[$subpro[0]][] = $subpro[1];
	}
	$proDiv .='<div>';
	foreach($r as $key => $val)
	{
		$sqlCat = getSelectList(TBL_CATEGORY,"cat_",$auto_id=$key,"Active");
		$resCat = $db->select($sqlCat);
		
		$proDiv .='<div class="contact-text">'.$resCat[0]["cat_name"].'</div>';
		for($i=0;$i<count($r[$key]);$i++)
		{
			$sqlSubCat = getSelectList(TBL_CATEGORY,"cat_",$auto_id=$r[$key][$i],"Active");
			$sqlSubCat .=" AND cat_parent_id = '".$key."'";
			$resSubCat = $db->select($sqlSubCat);
			$proDiv .='<div style="margin-left:16px;">'.$resSubCat[0]["cat_name"].'</div>';
		}
	}
	$proDiv .='</div>';
	return $proDiv;
}

function checkValueExist($table,$field_prefix,$field_name,$field_value,$id="")
{
	global $db;

	$sql_query = getSelectList($table,$field_prefix);
	$sql_query .=" AND ".$field_prefix.$field_name." = '".$field_value."'";
	
	if(!empty($id))
	{
		$sql_query .=" AND ".$field_prefix."id != '".$id."'";
	}
	$sql_query .=

	$resValue = $db->select($sql_query);
	if(count($resValue) > 0)
	{
		return 1;
	}
	else
	{
		return 0;
	}
}

function checkedRadio($value1,$value2,$defult="")
{

	if(!empty($value1))
	{
		if($value1 == $value2)
		{
			$checked = 'checked="checked"';
		}
	}
	else if($defult == "1")
	{
		$checked = 'checked="checked"';
	}
	else
	{
		$checked = '';
	}
	return $checked;
}

function displayDefultValInlisting($value,$defultvalue="N/A",$chkVal="")
{
	if(!empty($value))
	{
		$defult = $value;
	}
	else if($chkVal == $value)
	{
		$defult = $defultvalue;
	}
	else
	{
		$defult = $defultvalue;
	}
	return $defult;
}

function implodeArry($arrVal,$explodeval)
{
	if(!empty($arrVal) && $arrVal > 1)
	{
		$implodeVal = implode($explodeval,$arrVal);
	}
	else
	{
		$implodeVal = $arrVal;
	}
	return $implodeVal;
}

function getAssignSupplierList($con_id,$implodeSign=",")
{
	global $db;

	$extra = "(SELECT sup_fname 
				FROM ".TBL_SUPPLIER." as sup
				WHERE sup_id = led_sup_id) as supName";
	$sql_query = getSelectList(TBL_SUPPLIER_LEADS,"led_",'','',$extra);
	$sql_query .=" AND led_con_id = '".$con_id."' AND led_status = 'Active'";
	
	$resSupp = $db->select($sql_query);
	
	for($i=0;$i<count($resSupp);$i++)
	{
		$arr[] = $resSupp[$i]["supName"];
	}
	if(count($resSupp) > 0)
	{
		if(!empty($arr) && count($arr) > 1)
		{
			$suppname = implodeArry($arr,$implodeSign);
		}
		else
		{
			$suppname = $arr[0];
		}
	}
	else
	{
		$suppname = "N/A";
	}
	return $suppname;
}


function getSimpleTopMenu($language_prefix)
{

	
    global $db;
	if($language_prefix != '_fr')
	{
		$list_menu_query            = getFrontTopMainMenu();
	}
	else
	{
		$list_menu_query            = getFrontTopMainMenuFr();
	}
	$result_menu_query          = $db->select($list_menu_query);
    $total_menu_rows            = count($result_menu_query);
	
	//echo "hi".$language_prefix;
	/**************About vinix sub menu  *************/
	$list_menu_query2 .= "SELECT * FROM ".TBL_FRONT_MENU." WHERE fmnu_display_type = 'other' AND fmnu_display = '1' AND fmnu_status = 'Active' AND fmnu_pid=2";
    $result_menu_query2 = $db->select($list_menu_query2);
	$array2 = array();	
	if($result_menu_query2!="") {
		$j=1;
		foreach($result_menu_query2 as $resultMenuQuery2Data)  {				
			if($j==1){			
				$array2 = array($resultMenuQuery2Data['fmnu_htaccess'.$language_prefix],'function');
			} else {
				$arrayVal = $resultMenuQuery2Data['fmnu_htaccess'.$language_prefix];
				$array3 = array_push($array2,$arrayVal);
			}
			$j++;
		}
		$aboutVinixArray = $array2;
		array_push($aboutVinixArray,"jan-moeremans","vincent-rotthier");
		
	}
	/**************End About vinix sub menu  *************/
	
	/**************Service sub menu  *************/
	$list_menu_query3 .= "SELECT * FROM ".TBL_FRONT_MENU." WHERE fmnu_display_type = 'other' AND fmnu_display = '1' AND fmnu_status = 'Active' AND fmnu_pid=3";
    $result_menu_query3 = $db->select($list_menu_query3);
	$arraySer2 = array();	
	if($result_menu_query3!="") {
		$i=1;
		foreach($result_menu_query3 as $resultMenuQuery3Data)  {				
			if($i==1){			
				$arraySer2 = array($resultMenuQuery3Data['fmnu_htaccess'.$language_prefix]);
			} else {
				$arraySerVal = $resultMenuQuery3Data['fmnu_htaccess'.$language_prefix];
				$arraySer3 = array_push($arraySer2,$arraySerVal);
			}
			$i++;
		}
		$serviceArray = $arraySer3;		
	}
	/**************End service sub menu  *************/
	
	if($total_menu_rows>0) {			
		
		$menuTop .='<ul>';
		 for($i=0;$i<$total_menu_rows;$i++) {
				if(SITE_URL_REWRITE == "On") {
				   	$link = $result_menu_query[$i]["fmnu_htaccess".$language_prefix];                    
				} else {
					$link = $result_menu_query[$i]["fmnu_page".$language_prefix];                    
				}
				$display_name = strtoupper($result_menu_query[$i]["fmnu_title".$language_prefix]);
				 $link2 = $result_menu_query[$i]["fmnu_page".$language_prefix];
				$width = $result_menu_query[$i]["fmnu_width"];
                
				if(empty($_REQUEST['files'])) {
					if($link2 == "home"){						
						$class = 'act';
					} else {
						$class = '';
					}
				} 
				else if($_REQUEST['files'] == $link2 && !empty($_REQUEST['files'])){
                    $class = 'act';
				}
				else {
					$class = '';
				}
               if(in_array($_REQUEST['files'],$aboutVinixArray)) {
                   if($link == "about-vinix"){
                        $class = 'act';
                   }else{
                         $extra = '';
                   }
                }
				 if($_REQUEST['files']=='form') {
                   if($link == "contact"){
                        $class = 'act';
                   }else{
                         $extra = '';
                   }
                }
				
				else if($_REQUEST['files'] == 'approach' || $_REQUEST['files'] == 'content-management-system-cms' || $_REQUEST['files'] == 'search-engine-optimization-seo') {
                   if($link == "services"){
                        $class = 'act';
                   }else{
                         $extra = '';
                   }
                }
				
				else if(in_array($_REQUEST['files'],$arraySer2)) {
                   if($link == "services"){
                        $class = 'act';
                   }else{
                         $extra = '';
                   }
                }
				else if($_REQUEST['files']=="client") {
                   if($link == "portfolio"){
                        $class = 'act';
                   }else{
                         $extra = '';
                   }
                }
				
                 if($link2 == "#contact")
      				{
      					
                            $link = '#';
      						$menuTop .='<li>';
      						$menuTop .=getFrontActionLink($link,$display_name,'','','',$display_name,$extra);
      						$menuTop .='</li>';
      					
      				} else {
							
                        $menuTop .='<li>';
      					$menuTop .=getFrontActionLink($link,$display_name,$class,'','',$display_name,$extra);
      					$menuTop .='</li>';
      				}

            }
			$menuTop .='</ul>';
		}
		return $menuTop;
}
function getProjectMenu($proNameId)
{
	global $db;
	
	$list_menu_query .= "SELECT * FROM ".TBL_FRONT_MENU." WHERE fmnu_display_type = 'other' AND fmnu_display = '1' AND fmnu_status = 'Active' AND fmnu_pid=".$proNameId." ORDER BY fmnu_sort_order";
    $result_menu_query  = $db->select($list_menu_query);
	
	$list_menu_query_main .= "SELECT * FROM ".TBL_FRONT_MENU." WHERE  fmnu_display = '1' AND fmnu_status = 'Active' AND fmnu_id=".$proNameId."";
    $result_menu_project_query  = $db->select($list_menu_query_main);
	$secname = $result_menu_project_query[0]['fmnu_htaccess'];
	
	$total_menu_rows = count($result_menu_query);	
	if($total_menu_rows>0)
	{
		$menuTop .='<ul>';
		$m=1;		
		for($i=0;$i<$total_menu_rows;$i++)
		{		
			if($m%2==0){
			$class = "left_menu_even";
			} else {
			$class = "left_menu_odd";
			}
			
			$display_name= strtoupper($result_menu_query[$i]["fmnu_title"]);
			if(SITE_URL_REWRITE == "On")
			{
				if($result_menu_query[$i]["fmnu_htaccess"] !="")
				{
					if(!empty($secname)) {
						if($result_menu_query[$i]["fmnu_htaccess"]=="about-vinix"){
							$link = $result_menu_query[$i]["fmnu_htaccess"];
						} else {
							
							$link = $secname."/".$result_menu_query[$i]["fmnu_htaccess"];
							$managementArr=array('vincent-rotthier','jan-moeremans');
							$webdesignArr=array('approach','content-management-system-cms','search-engine-optimization-seo');
							
							
							$careerArr = array('function');
							if(in_array($_GET["files"],$managementArr)){								
								if($result_menu_query[$i]["fmnu_htaccess"]=="management") {
									$class = "active";
								}
							}
							if(in_array($_GET["files"],$careerArr)){								
								if($result_menu_query[$i]["fmnu_htaccess"]=="career") {
									$class = "active";
								}
							}
							if(in_array($_GET["files"],$webdesignArr)){								
								if($result_menu_query[$i]["fmnu_htaccess"]=="web-design-and-development") {
									$class = "active";
								}
							}
							else if($result_menu_query[$i]["fmnu_htaccess"]==$_REQUEST['date']){								
								$class = "active";
							} else {								
								//$class = "";	
							}
							
						}
						
					}
					else
					{
						//$link = "projecten/".$proName;
						$link  = "";
					}
				}
				$menuTop .='<li>';			
				$menuTop .=getFrontActionLink($link,$display_name,$class,"","",$display_name);
				$menuTop .='</li>';												  			
				
			}
		$m++;	
		}
		$menuTop .='</ul>';
	}
	echo $menuTop;	
}

function getHtacesslinkName($title)
{
	$hts = strtolower(str_replace(" ","-",$title));
	return $hts;
}

function getURIinProjectName()
{
	global $db;
	
	$uri = explode("/",$_SERVER["REQUEST_URI"]);
	$tt = $uri[count($uri)-2];
	$sql = getSelectList(TBL_PROJECT,"pro_");
	$sql .= " AND LOWER(REPLACE(pro_title,' ','-')) = '".$tt."'";
	$res = $db->select($sql);
	
	$idArr = array();
	$idArr["sec_id"] = $res[0]["pro_sec_id"];
	$idArr["pro_id"] = $res[0]["pro_id"];
	
	return $idArr;
}

function frtshowerr()
{
	$str = '';
	if(!empty($_SESSION['error']))
	{
		$str = '<table width="100%" border="0" align="">';
		for($i=0; $i<count($_SESSION['error']); $i++)
		{
			$str .= "<tr><td style='color:#E71D07'><img src='images/error.gif' border='0'> ".$_SESSION['error'][$i]."</td></tr>";
		}
		$str .= "</table>";
		return $str;
	}
	return false;
}

function frtshowmsg()
{
	$str = '';
	if(!empty($_SESSION['msg']))
	{
		$str = '<table width="100%" border="0" align="">';
		for($i=0; $i<count($_SESSION['msg']); $i++)
		{
			$str .='<tr><td>'.$_SESSION['msg'][$i].'</td></tr>';
		}
		$str .= "</table>";
		return $str;
	}
	return false;
}

function getPrintLinkSection($titlelink,$proName)
{
	if(SITE_URL_REWRITE == "On")
	{
		$url = "projecten/".$proName."/hou-me-op-de-hoogte";
	}
	else
	{
		$url = FRT_HOU_ME_OP_DE_HOOGTE;
	}

	$printlink = '';
	$printlink .='<div class="sub-links-right">
						<ul>
							<li><a href="javascript:window.popPrint();" title="AFDRUKKEN">PROJECTFICHE AFDRUKKEN</a></li>
							<li><a href="'.getFrontURL($url).'" title="HOU ME OP DE HOOGTE">HOU ME OP DE HOOGTE</a></li>
						</ul>
					</div>';
                    //<li><a href="'.getFrontURL($url).'" title="HOU ME OP DE HOOGTE">HOU ME OP DE HOOGTE</a></li>
	return $printlink;					
}

function formatMoney($number, $fractional=false) {
    if ($fractional) {
        $number = sprintf('%.2f', $number);
    }
    while (true) {
        $replaced = preg_replace('/(-?\d+)(\d\d\d)/', '$1.$2', $number);
        if ($replaced != $number) {
            $number = $replaced;
        } else {
            break;
        }
    }
    return $number;
} 

function setSidePanelMenu()
{
	global $db;

	if(!empty($_GET["id"]))
	{
		$sql = getSelectList(TBL_PROJECT,"pro_");
		$sql .= " AND LOWER(REPLACE(pro_title,' ','-')) = '".$_GET["id"]."'";
		$res = $db->select($sql);
		$pro_id = $res[0]["pro_id"];
		$sec_id = $res[0]["pro_sec_id"];
	}
	else
	{
		$idArr = getURIinProjectName();
		$sec_id = $idArr["sec_id"];
		$pro_id = $idArr["pro_id"];
	}

	$sqllig = getSelectList(TBL_LIGGING,"lig_","","Active");
	$sqllig .= " AND lig_pro_id = '".$pro_id."'";
	$reslig = $db->select($sqllig);

	$sqlAan = getSelectList(TBL_AANBOD,"aan_","","Active");
	$sqlAan .= " AND aan_pro_id = '".$pro_id."'";
	$resAan = $db->select($sqlAan);

    $sqlPlannen = getSelectList(TBL_ARCHITECTUUR,"arc_","","Active");
	$sqlPlannen .= " AND arc_pro_id = '".$pro_id."'";
	$resPlannen = $db->select($sqlPlannen);

    $sqlDuu = getSelectList(TBL_DUURZAAM_ONTWIKKELE,"duu_","","Active");
	$sqlDuu .= " AND duu_pro_id = '".$pro_id."'";
	$resDuu = $db->select($sqlDuu);
	
	$sqlFoto = getSelectList(TBL_FOTO,"fot_","","Active");
	$sqlFoto .= " AND fot_pro_id = '".$pro_id."'";
	$resFoto = $db->select($sqlFoto);

    $sqlCon = getSelectList(TBL_CONTACT,"con_","","Active");
	$sqlCon .= " AND con_pro_id = '".$pro_id."'";
	$resCon = $db->select($sqlCon);


	if(empty($reslig))
	{
		$idsArr[] = 30;
	}
    if(empty($resAan))
	{
		$idsArr[] = 31;
	}
    if(empty($resPlannen))
	{
		$idsArr[] = 32;
	}
     if(empty($resDuu))
	{
		$idsArr[] = 33;
	}
    if(empty($resFoto))
	{
		$idsArr[] = 34;
	}
    if(empty($resCon))
	{
		$idsArr[] = 35;
	}
	return $idsArr;
}


function leftDuurzaamDisply($stringRequrireId){
  global $db;
  $sqlDuuzaamPage = getSelectList(TBL_PAGES,"pag_","","Active");
  $sqlDuuzaamPage .= " AND 	pag_fmid in ($stringRequrireId)";
  $resDuuzaamPageArr = $db->select($sqlDuuzaamPage);
  $resCount = count($resDuuzaamPageArr);
  $htmlOutput = ' <ul class="inner-menu-right">';
    $r=1;
     foreach($resDuuzaamPageArr as $resDuuzaamPage)  {
        $pagename =  $resDuuzaamPage['pag_url'];
        if($r==$resCount){$rightLastClass = 'inner-menu-right-last';}else{$rightLastClass = '';}
        if(SITE_URL_REWRITE!='On')  {
    		$linkDuuzaam = FRT_FOLDER_WWW.FRT_INDEX_PARAMETER.$pagename;
	    } else {
           	$linkDuuzaam=FRT_FOLDER_WWW.'nl/duurzaam-ontwikkelen/'.$pagename;
        }
       $imagePath = PAGE_IMG_DISPLAY.$resDuuzaamPage['pag_image'];
        $htmlOutput .= '<li class="'.$rightLastClass.'"><span>';
        if($resDuuzaamPage['pag_image']!="") {
            $htmlOutput .='<a href="'.$linkDuuzaam.'"><img title="'.$resDuuzaamPage['pag_title'].'" alt="'.$resDuuzaamPage['pag_title'].'" src="'.$imagePath.'" width="64px" height="61px"></a>';
        }
       $htmlOutput .= ' </span><a href="'.$linkDuuzaam.'" title="'.$resDuuzaamPage['pag_title'].'">'.ucfirst($resDuuzaamPage['pag_title']).'</a><br>
        '.$resDuuzaamPage['pag_short_desc'].'<br clear="all"></li>';
    $r++;
    }
     $htmlOutput .= '</ul>';
    return $htmlOutput;

}

function displayError($err,$errcnt)
{
	if(!empty($err))
	{
		$display='<tr>
					  <td class="msg_box" align="left" colspan=2>
					  ';
			 
					  for($j=0;$j <$errcnt; $j++) 
					  { 				
						$display.='<font style=font-size:15px;font-weight:normal;color:#FF0000>'.($j+1).'.&nbsp;'.$err[$j].'</font><br/>';
					  }
			 		  
			$display.='</td>
				</tr>
				';
		
		return $display;
	} 
}
