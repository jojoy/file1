function getAdminDeleteLink($tableName,$fieldPrefix,$fieldwhere,$displayName,$xtraCondition='')
{
	$link='<a href="javascript:;" onClick="getAjaxDeleteAction(\''.$tableName.'\',\''.$fieldPrefix.'\',\''.$fieldwhere.'\',\''.$displayName.'\',\''.$xtraCondition.'\');">Delete</a>';
	
	return $link;	
}

function getAdminActionLink($pagename,$language_prefix,$action,$id_name,$id,$display_name="",$xtra="")
{
	if(!empty($id))
	{
		$getid='&'.$id_name.'='.$id;
	}
	
	if($display_name=="Active")
	{
		$status='&status=Inactive';
	}
	else if($display_name=="Inactive")
	{
		$status="&status=Active";
	}
	
	if($display_name=="Header")
	{
		$status='&location=Footer';
	}
	else if($display_name=="Footer")
	{
		$status="&location=Header";
	}
	
	
	if($display_name=="")
	{
		$display_name=$action;
	}
	
	if($action=="Images")
	{
		$link='<a href="'.ADM_FOLDER_WWW.ADM_INDEX_PARAMETER.$pagename.'&method='.$action.$getid.$status.$xtra.'">'.$display_name.'</a>';
		/*.'&language_prefix='.$language_prefix*/
	}
	else if($action=="Delete")
	{
		//$link='<a href="'.ADM_FOLDER_WWW.ADM_INDEX_PARAMETER.$pagename.'&method='.$action.$getid.$status.$xtra.'" onClick="return confirm(\'Are you sure want to delete?\');">'.$display_name.'</a>';
		$link='<a href="'.ADM_FOLDER_WWW.ADM_INDEX_PARAMETER.$pagename.'&language_prefix='.$language_prefix.'&method='.$action.$getid.$status.$xtra.'" onClick="return confirm(\'Are you sure want to delete?\');"><img src="'.ADM_IMAGES_WWW.'delete.gif" alt="Delete" title="Delete" border=0></a>';
		/*.'&language_prefix='.$language_prefix*/
	}
	else if($action=="Edit")
	{
		$link='<a href="'.ADM_FOLDER_WWW.ADM_INDEX_PARAMETER.$pagename.'&language_prefix='.$language_prefix.'&method='.$action.$getid.$status.$xtra.'"><img src="'.ADM_IMAGES_WWW.'edit.png" alt="Edit" title="Edit" border=0></a>';
	}
	else if($action=="EditModule")
	{
		$link='<a href="'.ADM_FOLDER_WWW.ADM_INDEX_PARAMETER.$pagename.'"><img src="'.ADM_IMAGES_WWW.'edit.png" alt="Edit" title="Edit" border=0></a>';
	}
	else if($display_name=="Active")
	{
		$link='<a href="'.ADM_FOLDER_WWW.ADM_INDEX_PARAMETER.$pagename.'&method='.$action.$getid.$status.$xtra.'"><img src="'.ADM_IMAGES_WWW.'active_status.png" alt="Active" title="Active" border=0></a>';
	}
	else if($display_name=="Inactive")
	{
		$link='<a href="'.ADM_FOLDER_WWW.ADM_INDEX_PARAMETER.$pagename.'&method='.$action.$getid.$status.$xtra.'"><img src="'.ADM_IMAGES_WWW.'inactive.png" alt="Inactive" title="Inactive"  border=0></a>';
	}
	else
	{
		$link='<a href="'.ADM_FOLDER_WWW.ADM_INDEX_PARAMETER.$pagename.'&method='.$action.$getid.$status.$xtra.'">'.$display_name.'</a>';
		
		
		/*.'&language_prefix='.$language_prefix*/
	}
	return $link;	
}

function getAdminURL($pagename,$language_prefix="",$action="",$select_id="")
{
	$URL = ADM_FOLDER_WWW.ADM_INDEX_PARAMETER.$pagename;
	
	/*if(isset($language_prefix))
	{
		$URL .= "&language_prefix=".$language_prefix;
	}*/
	if(!empty($action))
	{
		$URL .= "&method=".$action;
	}
	if(!empty($select_id))
	{
		$URL .= "&".$select_id;
	}
	
	return $URL;
}

function getAdminMiddleURL($pagename,$language_prefix="",$extra="")
{
	$URL = ADM_FOLDER_WWW.$pagename.$extra;
	/*if(isset($language_prefix))
	{
		$URL .= "&language_prefix=".$language_prefix;
	}*/
	
	return $URL;	
}
